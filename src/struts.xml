        	<result name="edit">/jsp/acde/midteamEdit.jsp</result>
        	<result name="delete">/jsp/midteam.jsp</result>
        	<result name="left">/left.jsp</result>
        </action>
        
        <action name="accidentrecordAction" class="accidentrecordAction">
